import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        HashMap<Integer,Integer> hash = new HashMap<>();
        Scanner sc = new Scanner(System.in);
        int n =sc.nextInt();
        int arr[] = new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
            hash.put(arr[i],hash.getOrDefault(arr[i],0)+1);
        }
        
        
        System.out.print(hash);
 
        }
    }
